{
  "name": "moolah",
  "version": "0.0.1",
  "description": "Track your finances.",
  "main": "client-dist/app.js",
  "scripts": {
    "test": "gulp test",
    "test:unit": "gulp test:unit",
    "test:api": "gulp test:api:integration",
    "pretest": "dropdb moolah_test_db -U postgres || true && createdb moolah_test_db -U postgres",
    "pretest:api": "dropdb moolah_test_db -U postgres || true && createdb moolah_test_db -U postgres",
    "precoverage": "dropdb moolah_test_db -U postgres || true && createdb moolah_test_db -U postgres",
    "lint": "gulp lint",
    "test-browser": "gulp test-browser",
    "watch-tests": "gulp watch-tests",
    "build": "gulp build",
    "coverage": "BABEL_ENV=tests gulp coverage",
    "work": "gulp work",
    "local-server": "nodemon --watch server server/index.js | bunyan -o short",
    "postinstall": "if [ $BUILD_ASSETS ]; then npm run build-assets; fi",
    "build-assets": "gulp build",
    "migrate:up": "careen -A -e",
    "migrate:down": "careen -R -e",
    "migrate:create": "careen -C",
    "release": "heroku pipelines:promote -r staging",
    "minify-emoji": "imagemin server/static/sheet_apple_64.png --plugin=pngquant --out-dir=server/static"
  },
