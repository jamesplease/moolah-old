  "description": "Track your finances.",
  "main": "client-dist/app.js",
  "scripts": {
    "test": "gulp && npm run test:api",
    "test:api": "gulp test:api:integration",
    "pretest:api": "dropdb test_finance_app -U postgres || true && createdb test_finance_app -U postgres",
    "lint": "gulp lint",
    "test-browser": "gulp test-browser",
    "watch-tests": "gulp watch-tests",
    "build": "gulp build",
    "coverage": "gulp coverage",
    "work": "gulp work",
    "local-server": "nodemon --watch server server/index.js | bunyan -o short",
